   "metadata": {
    "collapsed": false
   },
   "id": "modular-wages"
  },
  {
   "cell_type": "code",
   "execution_count": 260,
   "outputs": [],
   "source": [
    "admin_password = getpass(\"Admin password: \")\n",
    "keycloak = KeycloakClient(\n",
    "    server_url=server_url,\n",

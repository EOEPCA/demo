   "source": [
    "## User Management (Keycloak)\n",
    "Interaction with Keycloak using Keycloak's python client and Identity API."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 259,
   "outputs": [],
   "source": [
    "import jwt\n",
    "import json\n",
    "from getpass import getpass\n",
    "from keycloak import KeycloakPostError\n",
    "from identityutils.keycloak_client import KeycloakClient\n",
    "import requests\n",
    "import urllib3\n",
    "\n",
    "urllib3.disable_warnings()\n",
    "\n",
    "base_domain = \"demo.eoepca.org\"\n",
    "platform_domain = f\"identity.keycloak.{base_domain}\"\n",
    "server_url = f\"https://{platform_domain}\"\n",
    "realm = \"master\"\n",
    "dummy_service_url = f\"https://identity.dummy-service.{base_domain}\"\n",
    "identity_api_url = f\"https://identity.api.{base_domain}\""
   ],
   "metadata": {
    "collapsed": false,
    "ExecuteTime": {
     "end_time": "2023-12-19T08:57:38.329189Z",
     "start_time": "2023-12-19T08:57:38.311106800Z"
    }
